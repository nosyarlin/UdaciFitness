{
  "expo": {
    "sdkVersion": "20.0.0",
    "name": "UdaciFitness",
    "description": "The best triathlon tracker ever",
    "slug": "UdaciFitness",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://tylermcginnis.com/udacifitness-512.png",
    "notification": {
      "icon": "https://tylermcginnis.com/udacifitness-45.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.udacifitness.exp",
      "supportsTablet": true
    },
    "android": {
      "package": "org.udacifitness.exp",
    }
  }
}
